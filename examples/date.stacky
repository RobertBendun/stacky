"io"   import
"time" import

"Date is " print

2cell-print fun u64 -- is
	dup 10 < if "0" print end
	print-int
end

now # returns seconds in Unix time
date # returns year, month, day, hours, minutes
	print-int "-" print
	2cell-print "-" print
	2cell-print " " print
	2cell-print ":" print
	2cell-print
